##!/bin/csh

cd "$(dirname "$0")"

echo -------------------------------------------------------------------------------
echo Build example_font_converter
echo -------------------------------------------------------------------------------

export MACOSX_DEPLOYMENT_TARGET=10.8
c++ -force_cpusubtype_ALL -arch i386 -stdlib=libc++ -c -std=c++0x -nostdlib -fno-exceptions -fno-rtti -Wfloat-equal -Wshadow -Wall -Wextra -Wstrict-aliasing=2 -Wcast-align -Wcast-qual -Wcomma -Wdouble-promotion -Wformat=2 -ffunction-sections -fdata-sections -fno-common -pedantic -I ../../../includes/core-cpp/ -I ../../../includes/utils/ -O3 ../../../examples-cpp/example_font_converter/main.cpp
c++ -force_cpusubtype_ALL -dead_strip -arch i386 -stdlib=libc++ -o example_font_converter main.o ../../../engine/libs/i386-osx_32-static/clang/dtypestd.o -fno-exceptions -fno-rtti -Wfloat-equal -Wshadow -Wall -Wextra -Wstrict-aliasing=2 -Wcast-align -Wcast-qual -pedantic

echo Done.
echo -------------------------------------------------------------------------------
